<?xml version="1.0"?>
<robot name="raskar" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Constants for robot dimensions -->
  <xacro:property name="Pi"              value="3.1415926535897931"/>
  <xacro:property name="mass_body"       value="68.0"  /> <!-- Mass of car        -->
  <xacro:property name="mass_wheel"      value="1.8"   /> <!-- Mass of wheel      -->
  <xacro:property name="mass_lidar"      value="0.1"   /> <!-- Mass of lidar      -->
  <xacro:property name="width_body"      value="0.628" /> <!-- Width of car       -->
  <xacro:property name="length_body"     value="0.948" /> <!-- Length of car      -->
  <xacro:property name="height_body"     value="0.282" /> <!-- Height of car      -->
  <xacro:property name="diam_wheel"      value="0.127" /> <!-- Diameter of wheel  -->
  <xacro:property name="thickness_wheel" value="0.066" /> <!-- Thickness of wheel -->
  <xacro:property name="diam_lidar"      value="0.075" /> <!-- Angle of lidar     -->
  <xacro:property name="thickness_lidar" value="0.045" /> <!-- Thickness of wheel -->
  <xacro:property name="safety_offset"   value="1.25"  /> <!-- Percentage safety  -->

 
  <!-- Gazebo initializer -->
  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find raskar_urdf)/urdf/raskar_params.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find raskar_urdf)/urdf/materials.xacro"      />


  <!-- Used for fixing robot to Gazebo -->
  <!--link name="world"/>

  <joint name="fixed" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
  </joint-->

  <link name="base_link">
    <visual>
      <origin rpy="1.571 0 0" xyz="0 0 -0.075"/>
      <geometry>	
        <mesh filename="package://raskar_urdf/meshes/body_parts.stl" scale="0.001 0.001 0.001" />	
      </geometry>
      <material name="pale_grey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.25"/>
      <geometry>
        <!--box size="0.948 0.636 0.3"/-->
        <box size="0.998 0.678 0.35"/>
      </geometry>      
    </collision>
    <inertial>
      <mass value="${mass_wheel}"/>
      <inertia 
	  ixx="${mass_body / 12.0 * (height_body*height_body + length_body*length_body)}" ixy="0.0" ixz="0.0"
	  iyy="${mass_body / 12.0 * (width_body*width_body + length_body*length_body)}" iyz="0.0"
	  izz="${mass_body / 12.0 * (width_body*width_body + height_body*height_body)}"/>
    </inertial>
  </link>

  <link name="wheel_left_back">
    <visual>
      <origin rpy="1.571 0 0" xyz="0 0 0"/>
      <geometry>	
        <mesh filename="package://raskar_urdf/meshes/5_in_mecanum.stl" scale="0.001 0.001 0.001" />	
      </geometry>
      <material name="dark_yellow"/>
    </visual>
    <inertial>
      <mass value="${mass_wheel}"/>
      <inertia 
	  ixx="${mass_wheel / 12.0 * (3*diam_wheel*diam_wheel/4 + thickness_wheel)}" ixy="0.0" ixz="0.0"
	  iyy="${mass_wheel / 12.0 * (3*diam_wheel*diam_wheel/4 + thickness_wheel)}" iyz="0.0"
	  izz="${mass_wheel / 2.0 * (diam_wheel*diam_wheel/4)}"/>
     </inertial>
  </link>

  <joint name="base_link_to_wheel_left_back" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel_left_back"/>
    <origin rpy="0 0 0" xyz="-0.260 0.2670 0.0635"/>
    <dynamics damping="0.7"/>
  </joint>

  <link name="wheel_left_front">
    <visual>
      <origin rpy="1.571 0 0" xyz="0 0 0"/>
      <geometry>	
        <mesh filename="package://raskar_urdf/meshes/5_in_mecanum.stl" scale="0.001 0.001 0.001" />	
      </geometry>
      <material name="dark_yellow"/>
    </visual>
    <inertial>
      <mass value="${mass_wheel}"/>
      <inertia 
	  ixx="${mass_wheel / 12.0 * (3*diam_wheel*diam_wheel/4 + thickness_wheel)}" ixy="0.0" ixz="0.0"
	  iyy="${mass_wheel / 12.0 * (3*diam_wheel*diam_wheel/4 + thickness_wheel)}" iyz="0.0"
	  izz="${mass_wheel / 2.0 * (diam_wheel*diam_wheel/4)}"/>
    </inertial>
  </link>

  <joint name="base_link_to_wheel_left_front" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel_left_front"/>
    <origin rpy="0 0 0" xyz="0.260 0.2670 0.0635"/>
    <dynamics damping="0.7"/>
  </joint>

  <link name="wheel_right_front">
    <visual>
      <origin rpy="1.571 0 0" xyz="0 0 0"/>
      <geometry>	
        <mesh filename="package://raskar_urdf/meshes/5_in_mecanum.stl" scale="0.001 0.001 0.001" />	
      </geometry>
      <material name="dark_yellow"/>
    </visual>
    <inertial>
      <mass value="${mass_wheel}"/>
      <inertia 
	  ixx="${mass_wheel / 12.0 * (3*diam_wheel*diam_wheel/4 + thickness_wheel)}" ixy="0.0" ixz="0.0"
	  iyy="${mass_wheel / 12.0 * (3*diam_wheel*diam_wheel/4 + thickness_wheel)}" iyz="0.0"
	  izz="${mass_wheel / 2.0 * (diam_wheel*diam_wheel/4)}"/>
      </inertial>
  </link>

  <joint name="base_link_to_wheel_right_front" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel_right_front"/>
    <origin rpy="0 0 0" xyz="0.260 -0.2670 0.0635"/>
    <dynamics damping="0.7"/>
  </joint>

  <link name="wheel_right_back">
    <visual>
      <origin rpy="1.571 0 0" xyz="0 0 0"/>
      <geometry>	
        <mesh filename="package://raskar_urdf/meshes/5_in_mecanum.stl" scale="0.001 0.001 0.001" />	
      </geometry>
      <material name="dark_yellow"/>
    </visual>
    <inertial>
      <mass value="${mass_wheel}"/>
      <inertia 
	  ixx="${mass_wheel / 12.0 * (3*diam_wheel*diam_wheel/4 + thickness_wheel)}" ixy="0.0" ixz="0.0"
	  iyy="${mass_wheel / 12.0 * (3*diam_wheel*diam_wheel/4 + thickness_wheel)}" iyz="0.0"
	  izz="${mass_wheel / 2.0 * (diam_wheel*diam_wheel/4)}"/>
    </inertial>
  </link>

  <joint name="base_link_to_wheel_right_back" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel_right_back"/>
    <origin rpy="0 0 0" xyz="-0.260 -0.2670 0.0635"/>
    <dynamics damping="0.7"/>
  </joint>

  <!--link name="rplidar_a2_front">echo 'LC_NUMERIC="en_US.UTF-8"' | sudo tee -a /etc/default/locale
    <visual>
      <origin rpy="1.571 0 3.1415" xyz="0 0 0"/>
      <geometry>	
        <mesh filename="package://raskar_urdf/meshes/rplidar_a2m4.stl" scale="0.000417 0.000417 0.000417" />	
      </geometry>
      <material name="shiny_black"/>
    </visual>
    <inertial>
      <mass value="${mass_lidar}"/>
      <inertia 
	  ixx="${mass_lidar / 12.0 * (3*diam_lidar*diam_lidar/4 + thickness_lidar)}" ixy="0.0" ixz="0.0"
	  iyy="${mass_lidar / 12.0 * (3*diam_lidar*diam_lidar/4 + thickness_lidar)}" iyz="0.0"
	  izz="${mass_lidar / 2.0 * (diam_lidar*diam_lidar/4)}"/>
      </inertial>
  </link>

  <joint name="base_link_to_rplidar_a2_front" type="fixed">
    <parent link="base_link"/>
    <child link="rplidar_a2_front"/>
    <origin rpy="0 0 -0.7853" xyz="0.415 -0.335 0.15"/>    
  </joint-->




  <transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="base_link_to_wheel_left_back">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="base_link_to_wheel_left_front">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor2">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="base_link_to_wheel_right_front">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor3">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="tran4">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="base_link_to_wheel_right_back">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor4">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>





</robot>
